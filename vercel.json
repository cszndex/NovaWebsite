{
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/app" }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "key.novateam.xyz"
        }
      ],
      "destination": "https://key.novateam.xyz/getkey"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "api.novateam.xyz"
        }
      ],
      "destination": "https://api.novateam.xyz/endpoint/"
    }
  ]
}
