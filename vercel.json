{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/app"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "key.novateam.xyz"
        }
      ],
      "destination": "https://novateam.xyz/getkey"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "api.novateam.xyz"
        }
      ],
      "destination": "https://api.novateam.xyz/endpoint"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "dex.novateam.xyz"
        }
      ],
      "destination": "https://dex.novateam.xyz/utility"
    }
  ]
}
